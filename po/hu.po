#: ../src/server/options.c:64
#, c-format
msgid "Usage: "
msgstr "Használat: "

#: ../src/server/options.c:66
#, c-format
msgid ""
"%s -- Common interface for Speech Synthesis %s\n"
"\n"
msgstr ""
"%s -- Általános interfész a beszédszintetizátorokhoz %s\n"
"\n"

#: ../src/server/options.c:68
#, c-format
msgid "Run as a daemon\n"
msgstr "Futtatás kiszolgálóként\n"

#: ../src/server/options.c:70
#, c-format
msgid "Run as single application\n"
msgstr "Futtatás önálló alkalmazásként\n"

#: ../src/server/options.c:72
#, c-format
msgid "Start only if autospawn is not disabled\n"
msgstr ""
"Indítás csak akkor, ha az autospawn beállítás nincs \n"
" letiltva\n"

#: ../src/server/options.c:74
#, c-format
msgid "Set log level (between %d and %d)\n"
msgstr "A naplózás szintjének beállítása (%d és %d között)\n"

#: ../src/server/options.c:76
#, c-format
msgid "Set path to logging\n"
msgstr "A naplófájlok elérési útjának megadása\n"

#: ../src/server/options.c:78
#, c-format
msgid "Communication method to use ('%s' or '%s')\n"
msgstr ""
"A kommunikációs mód megadása \n"
"('%s' vagy '%s')\n"

#: ../src/server/options.c:80
#, c-format
msgid "Socket path to use for '%s' method (filesystem path or '%s')\n"
msgstr ""
"Használt socket elérési út az '%s' módhoz \n"
"(fájlrendszer elérési útja, vagy a '%s' elérési út használata)\n"

#: ../src/server/options.c:82
#, c-format
msgid "Specify a port number for '%s' method\n"
msgstr "Egy portszám megadása az '%s' módhoz\n"

#: ../src/server/options.c:84
#, c-format
msgid "Set path to pid file\n"
msgstr "A pid fájl elérési útjának megadása\n"

#: ../src/server/options.c:86
#, c-format
msgid "Set path to configuration\n"
msgstr "A konfigurációs fájlok elérési útjának megadása\n"

#: ../src/server/options.c:88
#, c-format
msgid "Report version of this program\n"
msgstr "Kiírja a program verziószámát\n"

#: ../src/server/options.c:90
#, c-format
msgid "Output debugging information into %s\n"
msgstr ""
"A hibakövető információk mentése a \n"
"%s könyvtárba\n"

#: ../src/server/options.c:92 ../src/clients/say/options.c:129
#, c-format
msgid "Print this info\n"
msgstr "Megjeleníti az opciók leírását\n"

